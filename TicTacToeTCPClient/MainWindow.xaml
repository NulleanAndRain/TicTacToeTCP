<Window x:Class="TicTacToeTCPClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToeTCPClient"
        mc:Ignorable="d"
        Title="TicTacToe" Height="450" Width="800">
    <Grid>
        <Label Content="Server IP: " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="58"/>
        <TextBox x:Name="_IP_text" HorizontalAlignment="Left" Height="23" Margin="73,14,0,0" TextWrapping="Wrap" Text="127.0.0.1" VerticalAlignment="Top" Width="120"/>
        <Label Content="Port:" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Width="58"/>
        <TextBox x:Name="_Port_text" HorizontalAlignment="Left" Height="22" Margin="73,46,0,0" TextWrapping="Wrap" Text="25656" VerticalAlignment="Top" Width="120"/>
        <Label Content="Name: " HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" Width="58"/>
        <TextBox x:Name="_Name_text" HorizontalAlignment="Left" Height="22" Margin="73,77,0,0" TextWrapping="Wrap" Text="Чилипиздрик" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="_Connect_Btn" Content="Connect" HorizontalAlignment="Left" Margin="118,104,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button  Content="Button" HorizontalAlignment="Left" Margin="544,150,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Label x:Name="test_area" Content="Label" HorizontalAlignment="Left" Margin="252,26,0,0" VerticalAlignment="Top" Height="98" Width="442"/>

    </Grid>
</Window>
